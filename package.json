{
  "name": "@bizsim-game-studios/colyseus-schema",
  "version": "3.0.76",
  "description": "Binary state serializer with delta encoding for games (BizSim fork)",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    }
  },
  "scripts": {
    "build:schema": "node ./build-schema.mjs",
    "build": "node ./build.mjs",
    "watch": "node ./build.mjs --watch",
    "test": "mocha ./test/*.ts -r tsx --timeout 10000 --exit",
    "test:watch": "mocha ./test/*.ts -r tsx --timeout 10000 --watch",
    "coverage": "c8 npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BizSim-Game-Studios/colyseus-schema.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "serialization",
    "serializer",
    "websocket",
    "binary",
    "delta",
    "colyseus"
  ],
  "author": "Endel Dreyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BizSim-Game-Studios/colyseus-schema/issues"
  },
  "homepage": "https://colyseus.io/",
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "c8": "^10.1.3",
    "esbuild": "^0.19.9",
    "mocha": "^10.2.0",
    "nanoid": "^3.3.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
